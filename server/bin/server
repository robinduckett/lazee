#!/usr/bin/env coffee

dnode = require('dnode')

d = dnode.connect(5001)

d.on 'remote', (remote) ->
    remote.process
        type: 'unrar'
        path: '/Users/robin/Downloads/Wreck-It.Ralph.2012.DVDRip.XviD-RiPS/wreck-it.ralph.dvdrip.xvid.rar'
        dest: '/Users/robin/dev/lazee/tmp/wreck-it.ralph.dvdrip.xvid'
        created: new Date()
        
    process.exit()